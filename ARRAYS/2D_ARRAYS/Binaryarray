import java.util.*;
public class Binaryarray {
    public static void binary(int n, int arr[])
    {
        for(int i=0;i<n;i++)
        {
            int arr1=arr[i]%2;
            System.out.print(arr1+" ");
        }
        
    }
   public static void main(String[] args)
   {
    Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();

        int arr[]=new int[n];
        for(int i=0;i<n;i++)
        {
            arr[i]=sc.nextInt();
        }
        binary(n, arr);
   } 
}
